<style>
td{
	width: : 5000px;
	border:solid black 2px;
	padding : 10px;
}
</style>



<div class="container">
	<h3>Users</h3>
	<table class='table'>
		<tr>
			<th>ID</th>
			<th>Name</th>
			<th>Title</th>
			<th>Email</th>
			<th></th>
			<th></th>
			<th></th>
		</tr>


		<% _.each(users, function(user) { %>
		<tr data-id="<%= user.id %>" data-model="user" style="border:solid black 2px">
			<td><%= user.id %>    </td>
			<td><%= user.name %>  </td>
			<td><%= user.title %> </td>
			<td><%= user.email %> </td>
			<td><%= user.encryptedPassword %> </td>
			<td><a href="/user/show/<%= user.id %>">Show</a></td>
			<td><a href="/user/edit/<%= user.id %>">Edit</a></td>
			<td><a href="/user/destroy/<%= user.id %>">Delete</a></td>
		</tr>

		<% }) %>
	</table>
</div>
